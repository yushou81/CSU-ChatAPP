              <Image url="@../images/profile_picture.png" />
      </image>
       </ImageView>
   
       <Button id="voiceButton" layoutX="40.0" layoutY="700.0" mnemonicParsing="false" prefHeight="81.0" prefWidth="81.0" styleClass="button">
