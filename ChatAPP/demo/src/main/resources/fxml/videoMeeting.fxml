<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="776.0" prefWidth="984.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"

            fx:controller="com.ys.controller.VideoMeetingController">

    <ImageView fitHeight="60.0" fitWidth="120.0" layoutX="570.0" layoutY="5" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../images/video_meeting.png"/>
    </ImageView>

    <Rectangle x="20" y="55" width="1144" height="1.7" fill="darkgray"/>

    <ImageView fx:id="image" fitHeight="200.0" fitWidth="200.0" layoutX="532.0" layoutY="200.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../images/profile_picture.png"/>
    </ImageView>

    <Button fx:id="voiceButtonYes" onAction="#voiceclick1" layoutX="40.0" layoutY="700.0" mnemonicParsing="false" prefHeight="81.0" prefWidth="81.0"
            styleClass="button" >
        <graphic>
            <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../images/open_voice.png"/>
            </ImageView>
        </graphic>
    </Button>

    <Button fx:id="voiceButtonNO" onAction="#voiceclick" layoutX="40.0" layoutY="700.0" mnemonicParsing="false" prefHeight="81.0" prefWidth="81.0"
            styleClass="button" >
        <graphic>
            <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../images/cancel_voice.png"/>
            </ImageView>
        </graphic>
    </Button>

    <Label layoutX="58.0" layoutY="790.0" prefHeight="33.0" prefWidth="76.0" text="语音开关" >
        <font>
            <Font size="18.0" />
        </font>
    </Label>

    <Button fx:id="cameraoOButton" onAction="#Openview" layoutX="180.0" layoutY="700.0" mnemonicParsing="false" prefHeight="81.0" prefWidth="81.0"
            styleClass="button">
        <graphic>
            <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../images/open_view.png"/>
            </ImageView>
        </graphic>
    </Button>


    <Button fx:id="cameraButton" onAction="#Closeview" layoutX="180.0" layoutY="700.0" mnemonicParsing="false" prefHeight="81.0" prefWidth="81.0"
            styleClass="button">

        <graphic>
            <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../images/camera1.png"/>
            </ImageView>
        </graphic>
    </Button>

    <Label layoutX="200.0" layoutY="790.0" prefHeight="33.0" prefWidth="76.0" text="视频开关" >
        <font>
            <Font size="18.0" />
        </font>
    </Label>


    <Button fx:id="endButton" onAction="#End" layoutX="1020.0" layoutY="710" mnemonicParsing="false" prefHeight="80.0"

            prefWidth="160.0" styleClass="button">
        <graphic>
            <ImageView fitHeight="80.0" fitWidth="160.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../images/end.png"/>
            </ImageView>
        </graphic>
    </Button>
</AnchorPane>
