<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="840.0" prefWidth="1264.0" style="-fx-background-color: #a9d4e1;-fx-shadow-radius: 10; -fx-shadow-color: #888888;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ys.controller.OpenLiveController">
   <children>
       <Label layoutX="525.0" layoutY="29.0" prefHeight="70.0" prefWidth="200.0" text="直播广场">
           <font>
               <Font size="50.0" />
           </font>
           <style>
               -fx-text-fill: #35889a;
               -fx-font-weight: bold;
           </style>
       </Label>

      <Button fx:id="open" layoutX="564.0" layoutY="308.0" mnemonicParsing="false" prefHeight="122.0" prefWidth="127.0" style="-fx-background-radius: 15; -fx-background-color: linear-gradient(to bottom, #35889a, #2c7a7b); -fx-text-fill: white; -fx-cursor: hand;">
          <graphic>
              <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                      <Image url="@../images/open_icon.png" />
                  </image>
              </ImageView>
          </graphic>
      </Button>


       <Button fx:id="cancel" layoutX="1197.0" layoutY="5.0" mnemonicParsing="false" onAction="#Hide" prefHeight="50.0" prefWidth="50.0" style="-fx-background-radius: 15; -fx-background-color: linear-gradient(to bottom, #35889a, #2c7a7b); -fx-text-fill: white; -fx-cursor: hand;">
           <graphic>
               <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@../images/cancel_icon.png" />
                   </image>
               </ImageView>
           </graphic>
       </Button>

       <Label layoutX="602.0" layoutY="484.0" prefHeight="71.0" prefWidth="78.0" text="开播">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
